				[fire_event]
					name="mirroralertmessage"
				[/fire_event]
			[/command]
		[/set_menu_item]
	[/event]
	[event]
		name="mirroralertmessage"
		[if]
			[variable]
				equals="$p2_faction"
				name="p1_faction"
			[/variable]
			[then]
				[message]
					message="Mirror! Sorry."
					speaker="narrator"
				[/message]
			[/then]
			[else]
				[message]
					message="Not a mirror. Have fun!"
					speaker="narrator"
				[/message]
			[/else]
		[/if]
		[clear_menu_item]
			id="mirroralert"
		[/clear_menu_item]
	[/event]
