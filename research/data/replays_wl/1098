[replay_start]
	abbrev=""
	campaign=""
	campaign_define=""
	campaign_extra_defines=""
	campaign_type="multiplayer"
	can_end_turn=yes
	completion="running"
	current_time=0
#textdomain wesnoth-multiplayer
	description=_"Fight through corridors and gardens, and dominate the eastern courtyard of what used to be the glorious city of Elensefar."
	difficulty="NORMAL"
	end_credits=yes
	end_text=""
	end_text_duration=0
	experience_modifier=70
	force_lock_settings=""
	history=""
	id="multiplayer_elensefar_courtyard"
	init_side_done=yes
	label="2p — Elensefar Courtyard"
