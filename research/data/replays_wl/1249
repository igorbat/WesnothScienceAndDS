	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="the_city_falls.ogg"
	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="nunc_dimittis.ogg"
	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="revelation.ogg"
	[/music]
