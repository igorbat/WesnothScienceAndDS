					male_name=_"resilient"
					[effect]
						apply_to="hitpoints"
						increase_total=4
					[/effect]
					[effect]
						apply_to="hitpoints"
						increase_total=1
						times="per level"
					[/effect]
				[/trait]
			[/modifications]
		[/unit]
	[/side]
