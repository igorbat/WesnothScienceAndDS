					image="units/orcs/grunt-attack-[1~5].png:[100,75,100,50,75]"
				[/frame]
				[if]
					hits=yes
					[attack_sound_frame]
						sound="sword-1.ogg"
					[/attack_sound_frame]
				[/if]
				[else]
					hits=no
					[attack_sound_frame]
						sound="miss-[1~3].ogg"
					[/attack_sound_frame]
				[/else]
				[frame]
					image="units/orcs/grunt.png:75"
				[/frame]
			[/attack_anim]
			[death]
				start_time=0
				[frame]
					image="units/orcs/grunt-die-[1~8].png:120"
				[/frame]
			[/death]
			[defend]
				start_time=-126
				[frame]
					image="units/orcs/grunt-defend-1.png:1,units/orcs/grunt-defend-2.png:250,units/orcs/grunt-defend-1.png:1"
				[/frame]
				[if]
					hit_sound_start_time=-25
					hits="hit"
					[hit_sound_frame]
						sound="orc-hit-[1~4].ogg"
					[/hit_sound_frame]
				[/if]
			[/defend]
			[portrait]
				image="portraits/orcs/transparent/grunt.png"
				mirror=false
				side="left"
				size=400
			[/portrait]
			[portrait]
				image="portraits/orcs/transparent/grunt.png"
				mirror=true
				side="right"
				size=400
			[/portrait]
