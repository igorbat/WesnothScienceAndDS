					name="RNM_all_leaders[$i_temp].extra_recruit"
					value="$RNM_era_copy.multiplayer_side[$faction].extra_recruit"
				[/set_variable]
				[set_variable]
					name="RNM_all_leaders[$i_temp].variables.RNM_faction_choosen"
					value=yes
				[/set_variable]
				[unstore_unit]
					find_vacant=no
					variable="RNM_all_leaders[$i_temp]"
				[/unstore_unit]
				[transform_unit]
					id="$RNM_all_leaders[$i_temp].id"
					transform_to="$leader"
				[/transform_unit]
				[heal_unit]
					amount="full"
					[filter]
						id="$RNM_all_leaders[$i_temp].id"
					[/filter]
