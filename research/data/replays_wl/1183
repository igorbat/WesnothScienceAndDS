		mask=""
		name=_"First Watch"
		red=-45
		sound=""
	[/time]
	[time]
		blue=-10
		green=-35
		id="second_watch"
		image="misc/time-schedules/default/schedule-secondwatch.png"
		lawful_bonus=-25
		mask=""
		name=_"Second Watch"
		red=-45
		sound=""
	[/time]
