#textdomain wesnoth-help
					description=_"This attack poisons living targets. Poisoned units lose 8 HP every turn until they are cured or are reduced to 1 HP. Poison can not, of itself, kill a unit."
					id="poison"
					name=_"poison"
				[/poison]
			[/specials]
		[/attack]
