					[/filter_location]
				[/filter]
			[/hides]
		[/abilities]
		[attack_anim]
			sound_start_time=-100
			start_time=-450
			[filter_attack]
				name="axe"
			[/filter_attack]
			[if]
				hits=yes
				[sound_frame]
					sound="axe.ogg"
				[/sound_frame]
			[/if]
			[else]
				hits=no
				[sound_frame]
					sound="miss-[1~3].ogg"
				[/sound_frame]
			[/else]
			[if]
				direction="s,se,sw"
				[frame]
					image="units/undead-skeletal/skeleton/skeleton-se-melee[1~10].png:100"
				[/frame]
			[/if]
			[else]
				direction="n,ne,nw"
				[frame]
					image="units/undead-skeletal/skeleton/skeleton-ne-melee[1~10].png:100"
				[/frame]
			[/else]
		[/attack_anim]
		[death]
			start_time=0
			[frame]
				image="units/undead-skeletal/skeleton/skeleton-dying-[1~7].png:100"
			[/frame]
		[/death]
		[defend]
			start_time=-126
			[if]
				direction="s,se,sw"
				[frame]
					image="units/undead-skeletal/skeleton/skeleton-se-defend1.png:1,units/undead-skeletal/skeleton/skeleton-se-defend2.png:250,units/undead-skeletal/skeleton/skeleton-se-defend1.png:1"
				[/frame]
			[/if]
			[else]
				direction="n,ne,nw"
				[frame]
					image="units/undead-skeletal/skeleton/skeleton-ne-defend1.png:1,units/undead-skeletal/skeleton/skeleton-ne-defend2.png:250,units/undead-skeletal/skeleton/skeleton-ne-defend1.png:1"
				[/frame]
			[/else]
			[if]
				hit_sound_start_time=-25
				hits="hit"
				[hit_sound_frame]
					sound="skeleton-hit-[1~3].ogg"
				[/hit_sound_frame]
			[/if]
		[/defend]
		[idle_anim]
			start_time=0
			[filter]
				[not]
					[filter_wml]
						[status]
							poisoned=yes
						[/status]
					[/filter_wml]
				[/not]
			[/filter]
			[filter]
				[filter_adjacent]
					count=0
					is_enemy=yes
				[/filter_adjacent]
			[/filter]
