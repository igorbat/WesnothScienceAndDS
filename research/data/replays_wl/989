			level=2
			max_attacks=1
			max_experience=56
			max_hitpoints=55
			max_moves=5
			moves=5
			name="bridge_troll"
			overlays=""
			profile="portraits/drakes/transparent/flameheart.png"
			race="drake"
			random_traits=no
			resting=yes
			role=""
			side=2
			small_profile="portraits/drakes/flameheart.png"
			type="Drake Flare"
			undead_variation="drake"
			underlying_id=2
			unrenamable=no
			upkeep="full"
			usage="mixed fighter"
			variation=""
