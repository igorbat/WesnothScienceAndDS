					recruit="Elvish Fighter,Elvish Archer,Mage,Elvish Shaman,Elvish Scout,Wose,Merman Hunter"
					terrain_liked="Ff,Fa,Ss,Ww"
					type="random"
					[ai]
						recruitment_pattern="fighter,fighter,archer,archer,mixed fighter,healer,scout"
