							name=_"plague"
							type="Walking Corpse"
						[/plague]
					[/specials]
				[/attack]
				[defend]
					start_time=-126
					[frame]
						image="units/undead/soulless-wose.png:1,units/undead/soulless-wose-defend.png:250,units/undead/soulless-wose.png:1"
					[/frame]
					[if]
						hit_sound_start_time=-25
						hits="hit"
						[hit_sound_frame]
							sound="zombie-hit-[1~6].ogg"
						[/hit_sound_frame]
					[/if]
				[/defend]
				[death]
					start_time=0
					[frame]
						image="units/undead/soulless-wose-die-[1~4].png:150,units/undead/soulless-die-[5~10].png:150"
					[/frame]
				[/death]
				[attack_anim]
					direction="s"
					start_time=-200
					[filter_attack]
						name="touch"
					[/filter_attack]
					[frame]
						image="units/undead/soulless-wose-attack-s.png:400"
