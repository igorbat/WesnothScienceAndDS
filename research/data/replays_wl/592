				Wose=1
			[/killed]
			[attacks]
				[sequence]
					s00=1
					s01=2
					s1=1
					s10=1
					_num=30
				[/sequence]
				[sequence]
					s001=1
					s01=2
					s10=2
					s1001=1
					s11=1
					_num=40
				[/sequence]
