				name="i"
			[/variable]
			[do]
				[if]
					[variable]
						boolean_equals="yes"
						name="leaders_to_make_quick[$i].variables.dont_make_me_quick"
					[/variable]
					[then]
						[unstore_unit]
							variable="leaders_to_make_quick[$i]"
						[/unstore_unit]
					[/then]
					[else]
						[set_variables]
							name="temp"
							[literal]
								[trait]
#textdomain wesnoth
									female_name=_ "female^quick"
									id="quick"
									male_name=_ "quick"
									[effect]
										apply_to="movement"
										increase="1"
									[/effect]
