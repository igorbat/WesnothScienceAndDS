					[/frame]
				[/else]
				[else]
					direction="s"
					[frame]
						image="units/human-loyalists/horseman/horseman-s-attack[1~12].png:[100*3,70*9]"
					[/frame]
				[/else]
				[if]
					hits=yes
					[attack_sound_frame]
						sound="spear.ogg"
					[/attack_sound_frame]
				[/if]
				[else]
					hits=no
					[attack_sound_frame]
						sound="miss-[1~3].ogg"
					[/attack_sound_frame]
				[/else]
			[/attack_anim]
			[death]
				start_time=0
				[if]
					direction="sw,s,se"
					[frame]
						image="units/human-loyalists/horseman/horseman-se-die[1~5].png:100"
					[/frame]
				[/if]
				[else]
					direction="nw,n,ne"
					[frame]
						image="units/human-loyalists/horseman/horseman-ne-die[1~5].png:100"
					[/frame]
				[/else]
			[/death]
			[defend]
				start_time=-126
				[if]
					direction="s,se,sw"
					[frame]
						image="units/human-loyalists/horseman/horseman-se-defend1.png:1,units/human-loyalists/horseman/horseman-se-defend2.png:250,units/human-loyalists/horseman/horseman-se-defend1.png:1"
					[/frame]
				[/if]
