			ms_before=12000
			name="northerners.ogg"
		[/music]
		[music]
			append=yes
			ms_before=12000
			name="sad.ogg"
		[/music]
		[set_variable]
			name="i"
			value=0
		[/set_variable]
		[set_variable]
			name="REPEAT_i"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than=44
				name="REPEAT_i"
			[/variable]
			[do]
				[item]
					image="terrain/chasm/abyss.png"
					x="$i"
					y=0
				[/item]
				[set_variable]
					add=1
					name="i"
				[/set_variable]
				[set_variable]
					add=1
					name="REPEAT_i"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="REPEAT_i"
		[/clear_variable]
		[item]
			image="scenery/monolith1.png"
			x=15
			y=21
		[/item]
		[item]
			image="scenery/monolith1.png"
			x=20
			y=15
		[/item]
		[item]
			image="scenery/monolith3.png"
			x=23
