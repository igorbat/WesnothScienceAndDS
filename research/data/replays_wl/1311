	[event]
	[/event]
	[event]
		name="prestart"
		[store_unit]
			kill=yes
			variable="leaders_to_make_quick"
			[filter]
				canrecruit=yes
				[filter_wml]
					max_moves=4
				[/filter_wml]
			[/filter]
		[/store_unit]
		[set_variable]
			name="i"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than="$leaders_to_make_quick.length"
				name="i"
			[/variable]
			[do]
				[if]
					[variable]
						boolean_equals=yes
						name="leaders_to_make_quick[$i].variables.dont_make_me_quick"
					[/variable]
					[then]
						[unstore_unit]
							variable="leaders_to_make_quick[$i]"
						[/unstore_unit]
					[/then]
					[else]
						[set_variables]
							name="temp"
							[literal]
								[trait]
									female_name="female^quick"
									id="quick"
									male_name="quick"
									[effect]
										apply_to="movement"
										increase=1
									[/effect]
									[effect]
										apply_to="hitpoints"
										increase_total="-5%"
									[/effect]
								[/trait]
							[/literal]
						[/set_variables]
						[set_variables]
							mode="append"
							name="leaders_to_make_quick[$i].modifications.trait"
							[insert_tag]
								name="literal"
								variable="temp.trait"
							[/insert_tag]
						[/set_variables]
						[clear_variable]
							name="leaders_to_make_quick[$i].max_moves,leaders_to_make_quick[$i].moves,leaders_to_make_quick[$i].max_hitpoints,leaders_to_make_quick[$i].hitpoints"
						[/clear_variable]
						[unstore_unit]
							variable="leaders_to_make_quick[$i]"
						[/unstore_unit]
					[/else]
				[/if]
				[set_variable]
					add=1
					name="i"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i"
		[/clear_variable]
		[clear_variable]
			name="leaders_to_make_quick,temp"
		[/clear_variable]
	[/event]
	[event]
		name="prestart"
		[if]
			[variable]
				boolean_equals=true
				name="rby_set_message"
			[/variable]
			[then]
				[chat]
					message="/me can be created at http://rushedbyyetis.heroku.com/"
					speaker="RBY Custom"
				[/chat]
				[chat]
					message="$rby_set_info[1].val"
					speaker="RBY $rby_set_info[0].val 26"
				[/chat]
				[chat]
					message="/me ctrl-x to clear"
					speaker="Press"
				[/chat]
			[/then]
		[/if]
		[objectives]
			[objective]
				condition="win"
				description="Defeat enemy leader"
			[/objective]
		[/objectives]
	[/event]
	[event]
		name="prestart"
		[music]
			ms_before=12000
			name="into_the_shadows.ogg"
		[/music]
		[music]
			append=yes
			ms_before=12000
			name="northern_mountains.ogg"
		[/music]
		[music]
			append=yes
			ms_before=12000
			name="journeys_end.ogg"
		[/music]
		[music]
