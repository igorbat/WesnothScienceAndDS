	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="transience.ogg"
	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="the_king_is_dead.ogg"
	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="knalgan_theme.ogg"
	[/music]
	[music]
		append=yes
