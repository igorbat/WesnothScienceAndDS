	[event]
	[/event]
	[event]
		name="prestart"
		[store_unit]
			kill=yes
			variable="leaders_to_make_quick"
			[filter]
				canrecruit=yes
				[filter_wml]
					max_moves=4
				[/filter_wml]
			[/filter]
		[/store_unit]
		[set_variable]
			name="i"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than="$leaders_to_make_quick.length"
				name="i"
			[/variable]
			[do]
				[if]
					[variable]
						boolean_equals=yes
						name="leaders_to_make_quick[$i].variables.dont_make_me_quick"
					[/variable]
					[then]
						[unstore_unit]
							variable="leaders_to_make_quick[$i]"
						[/unstore_unit]
					[/then]
					[else]
						[set_variables]
							name="temp"
							[literal]
								[trait]
									female_name=_"female^quick"
									id="quick"
									male_name=_"quick"
									[effect]
										apply_to="movement"
										increase=1
									[/effect]
									[effect]
										apply_to="hitpoints"
										increase_total="-5%"
									[/effect]
								[/trait]
							[/literal]
						[/set_variables]
						[set_variables]
							mode="append"
							name="leaders_to_make_quick[$i].modifications.trait"
							[insert_tag]
								name="literal"
								variable="temp.trait"
							[/insert_tag]
						[/set_variables]
						[clear_variable]
							name="leaders_to_make_quick[$i].max_moves,leaders_to_make_quick[$i].moves,leaders_to_make_quick[$i].max_hitpoints,leaders_to_make_quick[$i].hitpoints"
						[/clear_variable]
						[unstore_unit]
							variable="leaders_to_make_quick[$i]"
						[/unstore_unit]
					[/else]
				[/if]
				[set_variable]
					add=1
					name="i"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i"
		[/clear_variable]
		[clear_variable]
			name="leaders_to_make_quick,temp"
		[/clear_variable]
	[/event]
	[event]
		name="time over"
		[lua]
			code="
                local _ = wesnoth.textdomain ""wesnoth-multiplayer""
                local function all_sides()
                    local function f(s, i)
                        i = i + 1
                        local t = wesnoth.sides[i]
                        return t and i, t
                    end
                    return f, nil, 0
                end

                local income_factor = 5

                local side_num = -1
                local total_score = -1
                local side_comparison = """"
                for side, team in all_sides() do
                    repeat -- ugly hack to convert 'break' into 'continue'
                    if team.__cfg.hidden then
                        break
                    end
                    local r, g, b = 255, 255, 255
                    if     team.__cfg.color == 1 then r, g, b = 255,   0,   0
                    elseif team.__cfg.color == 2 then r, g, b =   0,   0, 255
                    elseif team.__cfg.color == 3 then r, g, b =   0, 255,   0
                    elseif team.__cfg.color == 4 then r, g, b = 155,  48, 255
                    elseif team.__cfg.color == 5 then r, g, b =   0,   0,   0
                    elseif team.__cfg.color == 6 then r, g, b = 165,  42,  42
                    elseif team.__cfg.color == 7 then r, g, b = 255, 165,   0
                    elseif team.__cfg.color == 8 then r, g, b = 255, 255, 255
                    elseif team.__cfg.color == 9 then r, g, b =   0, 128, 128 end
                    if # wesnoth.get_units( { side = side } ) == 0 then
                        side_comparison = side_comparison ..
                            string.format( tostring( _ ""<span strikethrough='true' foreground='#%02x%02x%02x'>Side %d</span>"") .. ""\n"",
                                r, g, b, side)
                        break
                    end
                    local income = team.total_income * income_factor
                    local units = 0
                    -- Calc the total unit-score here
                    for i, unit in ipairs( wesnoth.get_units { side = side } ) do
                        if not unit.__cfg.canrecruit then
                            wesnoth.fire(""unit_worth"", { id = unit.id })
                            units = units + wesnoth.get_variable(""unit_worth"")
                        end
                    end
                    -- Up to here
                    local total = units + team.gold + income
                    side_comparison = side_comparison ..
                        string.format( tostring( _ ""<span foreground='#%02x%02x%02x'>Side %d</span>:  Income score = %d  Unit score = %d  Gold = %d"") .. ""\n"" .. tostring( _ ""Grand total: <b>%d</b>"") .. ""\n"",
                            r, g, b, side, income, units, team.gold, total)
                    if total > total_score then
                        color = string.format(""#%02x%02x%02x"", r, g, b)
                        side_num = side
                        total_score = total
                    end
                    until true -- end ugly hack
                end
                side_comparison = side_comparison ..
                    string.format( ""\n"" .. tostring( _ ""<span foreground='%s'>Side %d</span> has the advantage.""), color, side_num)
                wesnoth.fire(""message"", { message = side_comparison, speaker = ""narrator"", image = ""wesnoth-icon.png""})
            "
		[/lua]
	[/event]
	[event]
	[/event]
	[event]
		name="prestart"
		[lua]
			code=" wesnoth.set_variable(""RNM_era"", wesnoth.game_config.era) "
		[/lua]
		[set_variable]
			name="i_temp"
			value="$RNM_era.multiplayer_side.length"
		[/set_variable]
		[set_variable]
			name="i_temp"
			sub=1
		[/set_variable]
		[while]
			[variable]
				greater_than_equal_to=0
				name="i_temp"
			[/variable]
			[do]
				[if]
					[variable]
						equals="$empty_string"
						name="RNM_era.multiplayer_side[$i_temp].leader"
					[/variable]
					[then]
						[clear_variable]
							name="RNM_era.multiplayer_side[$i_temp]"
						[/clear_variable]
					[/then]
				[/if]
				[set_variable]
					name="i_temp"
					sub=1
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i_temp"
		[/clear_variable]
		[store_unit]
			variable="RNM_all_leaders"
			[filter]
				canrecruit=yes
				[not]
					[filter_wml]
						RNM_faction_choosen=yes
					[/filter_wml]
				[/not]
			[/filter]
		[/store_unit]
		[set_variable]
			name="i_temp"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than="$RNM_all_leaders.length"
				name="i_temp"
			[/variable]
			[do]
				[if]
					[variable]
						name="RNM_era_copy.multiplayer_side.length"
						numerical_equals=0
					[/variable]
					[then]
						[set_variables]
							name="RNM_era_copy"
							to_variable="RNM_era"
						[/set_variables]
					[/then]
				[/if]
				[set_variable]
					name="faction"
					rand="1..$RNM_era_copy.multiplayer_side.length"
				[/set_variable]
				[set_variable]
					name="faction"
					sub=1
				[/set_variable]
				[if]
					[variable]
						equals="$empty_string"
						name="RNM_era_copy.multiplayer_side[$faction].random_leader"
					[/variable]
					[then]
						[set_variable]
							name="leader"
							rand="$RNM_era_copy.multiplayer_side[$faction].leader"
						[/set_variable]
					[/then]
					[else]
						[set_variable]
							name="leader"
							rand="$RNM_era_copy.multiplayer_side[$faction].random_leader"
						[/set_variable]
					[/else]
				[/if]
				[clear_variable]
					name="RNM_all_leaders[$i_temp].moves"
				[/clear_variable]
				[clear_variable]
					name="RNM_all_leaders[$i_temp].max_moves"
				[/clear_variable]
				[clear_variable]
					name="RNM_all_leaders[$i_temp].modifications.trait"
				[/clear_variable]
				[set_variable]
					name="RNM_all_leaders[$i_temp].type"
					value="$leader"
				[/set_variable]
				[set_variable]
					name="RNM_all_leaders[$i_temp].extra_recruit"
					value="$RNM_era_copy.multiplayer_side[$faction].extra_recruit"
				[/set_variable]
				[set_variable]
					name="RNM_all_leaders[$i_temp].variables.RNM_faction_choosen"
					value=yes
				[/set_variable]
				[unstore_unit]
					find_vacant=no
					variable="RNM_all_leaders[$i_temp]"
				[/unstore_unit]
				[transform_unit]
					id="$RNM_all_leaders[$i_temp].id"
					transform_to="$leader"
				[/transform_unit]
				[heal_unit]
					amount="full"
					[filter]
						id="$RNM_all_leaders[$i_temp].id"
					[/filter]
				[/heal_unit]
				[set_recruit]
					recruit="$RNM_era_copy.multiplayer_side[$faction].recruit"
					side="$RNM_all_leaders[$i_temp].side"
				[/set_recruit]
				[clear_variable]
					name="RNM_era_copy.multiplayer_side[$faction],leader,faction"
				[/clear_variable]
				[set_variable]
					add=1
					name="i_temp"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i_temp"
		[/clear_variable]
		[set_variable]
			name="i_temp"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than="$RNM_era.event.length"
				name="i_temp"
			[/variable]
			[do]
				[if]
					[variable]
						equals="prestart"
						name="RNM_era.event[$i_temp].name"
					[/variable]
					[then]
						[insert_tag]
							name="command"
							variable="RNM_era.event[$i_temp]"
						[/insert_tag]
					[/then]
				[/if]
				[set_variable]
					add=1
					name="i_temp"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i_temp"
		[/clear_variable]
		[clear_variable]
			name="RNM_all_leaders,RNM_era_copy,RNM_era"
		[/clear_variable]
	[/event]
