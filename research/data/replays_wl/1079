	[event]
		name="prestart"
		[lua]
			code=" wesnoth.set_variable(""RNM_era"", wesnoth.game_config.era) "
		[/lua]
		[set_variable]
			name="i_temp"
			value="$RNM_era.multiplayer_side.length"
		[/set_variable]
		[set_variable]
			name="i_temp"
			sub=1
		[/set_variable]
		[while]
			[variable]
				greater_than_equal_to=0
				name="i_temp"
			[/variable]
			[do]
				[if]
					[variable]
						equals="$empty_string"
						name="RNM_era.multiplayer_side[$i_temp].leader"
					[/variable]
					[then]
						[clear_variable]
							name="RNM_era.multiplayer_side[$i_temp]"
						[/clear_variable]
					[/then]
				[/if]
				[set_variable]
					name="i_temp"
					sub=1
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i_temp"
		[/clear_variable]
		[store_unit]
			variable="RNM_all_leaders"
			[filter]
				canrecruit=yes
				[not]
					[filter_wml]
						RNM_faction_choosen=yes
					[/filter_wml]
				[/not]
			[/filter]
		[/store_unit]
		[set_variable]
			name="i_temp"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than="$RNM_all_leaders.length"
				name="i_temp"
			[/variable]
			[do]
				[if]
					[variable]
						name="RNM_era_copy.multiplayer_side.length"
						numerical_equals=0
					[/variable]
					[then]
						[set_variables]
							name="RNM_era_copy"
							to_variable="RNM_era"
						[/set_variables]
					[/then]
				[/if]
				[set_variable]
					name="faction"
					rand="1..$RNM_era_copy.multiplayer_side.length"
				[/set_variable]
				[set_variable]
					name="faction"
					sub=1
				[/set_variable]
				[if]
					[variable]
						equals="$empty_string"
						name="RNM_era_copy.multiplayer_side[$faction].random_leader"
					[/variable]
					[then]
						[set_variable]
							name="leader"
							rand="$RNM_era_copy.multiplayer_side[$faction].leader"
						[/set_variable]
					[/then]
					[else]
						[set_variable]
							name="leader"
							rand="$RNM_era_copy.multiplayer_side[$faction].random_leader"
						[/set_variable]
					[/else]
				[/if]
				[clear_variable]
					name="RNM_all_leaders[$i_temp].moves"
				[/clear_variable]
				[clear_variable]
					name="RNM_all_leaders[$i_temp].max_moves"
				[/clear_variable]
				[clear_variable]
					name="RNM_all_leaders[$i_temp].modifications.trait"
				[/clear_variable]
				[set_variable]
					name="RNM_all_leaders[$i_temp].type"
					value="$leader"
				[/set_variable]
				[set_variable]
					name="RNM_all_leaders[$i_temp].extra_recruit"
					value="$RNM_era_copy.multiplayer_side[$faction].extra_recruit"
				[/set_variable]
				[set_variable]
					name="RNM_all_leaders[$i_temp].variables.RNM_faction_choosen"
					value=yes
				[/set_variable]
				[unstore_unit]
					find_vacant=no
					variable="RNM_all_leaders[$i_temp]"
				[/unstore_unit]
				[transform_unit]
					id="$RNM_all_leaders[$i_temp].id"
					transform_to="$leader"
				[/transform_unit]
				[heal_unit]
					amount="full"
					[filter]
						id="$RNM_all_leaders[$i_temp].id"
					[/filter]
				[/heal_unit]
				[set_recruit]
					recruit="$RNM_era_copy.multiplayer_side[$faction].recruit"
					side="$RNM_all_leaders[$i_temp].side"
				[/set_recruit]
				[clear_variable]
					name="RNM_era_copy.multiplayer_side[$faction],leader,faction"
				[/clear_variable]
				[set_variable]
					add=1
					name="i_temp"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i_temp"
		[/clear_variable]
		[set_variable]
			name="i_temp"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than="$RNM_era.event.length"
				name="i_temp"
			[/variable]
			[do]
				[if]
					[variable]
						equals="prestart"
						name="RNM_era.event[$i_temp].name"
					[/variable]
					[then]
						[insert_tag]
							name="command"
							variable="RNM_era.event[$i_temp]"
						[/insert_tag]
					[/then]
				[/if]
				[set_variable]
					add=1
					name="i_temp"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i_temp"
		[/clear_variable]
		[clear_variable]
			name="RNM_all_leaders,RNM_era_copy,RNM_era"
		[/clear_variable]
	[/event]
