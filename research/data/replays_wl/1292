				_" or greater. This is required of all joining players."
			speaker="narrator"
		[/message]
		[set_variable]
			name="pya_bfw_version_suffices"
			value=no
		[/set_variable]
		[endlevel]
			result="defeat"
		[/endlevel]
	[/event]
