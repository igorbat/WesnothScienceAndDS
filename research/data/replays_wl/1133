		ms_after=0
		ms_before=12000
		name="transience.ogg"
	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="revelation.ogg"
	[/music]
	[music]
		append=yes
		ms_after=0
		ms_before=12000
		name="loyalists.ogg"
	[/music]
