				petrified=yes
			[/status]
			[attack]
				damage=32
#textdomain wesnoth-units
				description=_"fist"
				icon="attacks/fist-yeti.png"
				name="fist"
				number=2
				range="melee"
				type="impact"
			[/attack]
			[modifications]
				[trait]
#textdomain wesnoth-multiplayer
					description=_"This is a sculpture made of stone."
					id="remove_hp"
					name=_"statue"
