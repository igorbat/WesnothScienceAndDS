	can_end_turn=yes
	completion="running"
	current_tod=5
	description="Fight through corridors and gardens, and dominate the eastern courtyard of what used to be the glorious city of Elensefar."
	difficulty="NORMAL"
	end_text=""
	end_text_duration=0
	experience_modifier=70
	history=""
	id="multiplayer_elensefar_courtyard"
	label="2p — Elensefar Courtyard"
