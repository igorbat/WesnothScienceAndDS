#textdomain wesnoth-help
					_"

Special Notes:" +
					_" Foes who lose their life to the plague will rise again in unlife, unless they are standing on a village."
				die_sound="zombie-hit-[4~6].ogg"
				experience=24
				flag_rgb="magenta"
				hitpoints=21
				id="Walking Corpse"
				image="units/undead/zombie-dwarf.png"
				inherit=""
				level=0
				movement=3
				movement_type="dwarvishfoot"
#textdomain wesnoth-units
				name=_"Walking Corpse"
				race="undead"
				usage="fighter"
				variation_id="dwarf"
				variation_name=_"wc_variation^Dwarf"
				[defend]
					start_time=-126
					[frame]
						image="units/undead/zombie-dwarf.png:1,units/undead/zombie-dwarf-defend.png:250,units/undead/zombie-dwarf.png:1"
					[/frame]
					[if]
						hit_sound_start_time=-25
						hits="hit"
						[hit_sound_frame]
							sound="zombie-hit-[4~6].ogg"
						[/hit_sound_frame]
					[/if]
				[/defend]
				[death]
					start_time=0
					[frame]
						image="units/undead/zombie-dwarf-die-[1~4].png:150,units/undead/soulless-die-[5~10].png:150"
					[/frame]
				[/death]
				[attack_anim]
					direction="s"
					start_time=-200
					[filter_attack]
						name="touch"
					[/filter_attack]
					[frame]
						image="units/undead/zombie-dwarf-attack-s.png:400"
						sound="zombie-attack.wav"
					[/frame]
				[/attack_anim]
				[attack_anim]
					direction="n"
					start_time=-200
					[filter_attack]
						name="touch"
					[/filter_attack]
					[frame]
						image="units/undead/zombie-dwarf-attack-n.png:400"
						sound="zombie-attack.wav"
					[/frame]
				[/attack_anim]
				[attack_anim]
					direction="se,sw,ne,nw"
					start_time=-200
					[filter_attack]
						name="touch"
					[/filter_attack]
					[frame]
						image="units/undead/zombie-dwarf-attack.png:400"
						sound="zombie-attack.wav"
					[/frame]
				[/attack_anim]
				[trait]
					availability="musthave"
#textdomain wesnoth-help
					description=_"Fights normally during unfavorable times of day/night"
					female_name=_"female^fearless"
					id="fearless"
					male_name=_"fearless"
				[/trait]
				[resistance]
					arcane=140
				[/resistance]
				[attack]
					damage=6
#textdomain wesnoth-units
					description=_"touch"
					icon="attacks/touch-zombie.png"
					name="touch"
					number=2
					range="melee"
					type="impact"
					[specials]
						[plague]
#textdomain wesnoth-help
							description=_"When a unit is killed by a Plague attack, that unit is replaced with a Walking Corpse on the same side as the unit with the Plague attack. This doesn’t work on Undead or units in villages."
							id="plague"
							name=_"plague"
							type="Walking Corpse"
						[/plague]
					[/specials]
				[/attack]
				[movement_costs]
					deep_water=3
				[/movement_costs]
				[defense]
					deep_water=90
				[/defense]
			[/variation]
			[variation]
