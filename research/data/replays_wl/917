[replay_start]
	abbrev=""
	campaign=""
	campaign_define=""
	campaign_extra_defines=""
	campaign_type="multiplayer"
	can_end_turn=yes
	completion="running"
	current_time=0
#textdomain wesnoth-multiplayer
	description=_"These caves were once the lair of Chak’kso Ney’yks, a legendary Elder Basilisk. The petrified forms of his victims remain as monuments to his savage power."
	difficulty="NORMAL"
	end_credits=yes
	end_text=""
	end_text_duration=0
	experience_modifier=70
	force_lock_settings=""
	history=""
	id="multiplayer_Basilisk"
	init_side_done=yes
	label="2и - Пещеры Василиска"
