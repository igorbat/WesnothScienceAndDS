				"1.11.10" +
				_" or greater. This is required of all joining players."
			speaker="narrator"
		[/message]
		[set_variable]
			name="pya_bfw_version_suffices"
			value=no
		[/set_variable]
		[endlevel]
			result="defeat"
		[/endlevel]
	[/event]
	[event]
		first_time_only=no
		name="post_advance"
