	[event]
		name="prestart"
		[objectives]
			summary="Created by py6aka
		Russian Wesnoth Community
		www.wesnothlife.ru"
			[objective]
				condition="win"
#textdomain wl_mappack
				description=_"Kill the enemy leader"
			[/objective]
