[ai]

#textdomain wesnoth
		description=_"Multiplayer_AI^RCA AI"
		id="testing_ai_default"
		version=10710
		[stage]
			id="main_loop"
			name="testing_ai_default::candidate_action_evaluation_loop"
			[candidate_action]
				engine="cpp"
