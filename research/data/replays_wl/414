		level=1
		max_attacks=1
		max_experience=35
		max_hitpoints=1
		max_moves=0
		moves=0
#textdomain wesnoth-multiplayer
		name=_"Statue"
		overlays=""
		profile="portraits/monsters/transparent/scorpion.png~RIGHT()"
		race="monster"
		random_traits=no
		resting=no
		role=""
		side=3
		small_profile="portraits/monsters/scorpion.png~RIGHT()"
		type="Giant Scorpion"
		undead_variation=""
		underlying_id=31
		unrenamable=yes
		upkeep="full"
		usage="fighter"
		variation=""
		x=9
		y=24
		zoc=yes
		[defense]
			castle=40
			cave=60
			flat=60
			forest=50
			frozen=60
			fungus=50
			hills=50
			mountains=40
			reef=60
			sand=60
			shallow_water=70
			swamp_water=60
			village=40
		[/defense]
		[movement_costs]
			castle=1
			cave=2
			flat=1
			forest=2
			frozen=2
			fungus=2
			hills=3
			mountains=4
			reef=2
			sand=2
			shallow_water=3
			swamp_water=2
			village=1
		[/movement_costs]
		[resistance]
			arcane=150
			blade=90
			cold=120
			fire=200
			impact=30
			pierce=90
		[/resistance]
		[advancement]
#textdomain wesnoth-help
			description=_"Max HP bonus +3, Max XP +20%"
			id="amla_default"
			image="misc/icon-amla-tough.png"
			max_times=100
			strict_amla=yes
			[effect]
				apply_to="hitpoints"
				heal_full=yes
				increase_total=3
			[/effect]
			[effect]
				apply_to="max_experience"
				increase="20%"
			[/effect]
			[effect]
				apply_to="status"
				remove="poisoned"
			[/effect]
			[effect]
				apply_to="status"
				remove="slowed"
			[/effect]
		[/advancement]
		[variables]
		[/variables]
		[filter_recall]
		[/filter_recall]
		[status]
			petrified=yes
		[/status]
		[attack]
			damage=9
			defense_weight=4
#textdomain wesnoth-units
			description=_"sting"
			name="sting"
			number=1
			range="melee"
			type="pierce"
			[specials]
				[poison]
#textdomain wesnoth-help
					description=_"Poison:
This attack poisons living targets. Poisoned units lose 8 HP every turn until they are cured or are reduced to 1 HP. Poison can not, of itself, kill a unit."
					id="poison"
					name=_"poison"
				[/poison]
			[/specials]
		[/attack]
		[attack]
			damage=4
#textdomain wesnoth-units
			description=_"pincers"
			name="pincers"
			number=4
			range="melee"
			type="impact"
		[/attack]
		[modifications]
			[trait]
				id="remove_hp"
				[effect]
					apply_to="hitpoints"
					increase_total="-100%"
				[/effect]
				[effect]
					apply_to="movement"
					set=0
				[/effect]
			[/trait]
		[/modifications]
	[/unit]
