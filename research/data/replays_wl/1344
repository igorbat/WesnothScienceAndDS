				race="undead"
				usage="fighter"
				variation_id="gryphon"
				variation_name=_"wc_variation^Gryphon"
				[defend]
					start_time=-126
					[frame]
						image="units/undead/zombie-drake.png:1,units/undead/zombie-drake-defend.png:250,units/undead/zombie-drake.png:1"
					[/frame]
					[if]
						hit_sound_start_time=-25
						hits="hit"
						[hit_sound_frame]
							sound="zombie-hit-[4~6].ogg"
						[/hit_sound_frame]
					[/if]
				[/defend]
				[death]
					start_time=0
					[frame]
						image="units/undead/zombie-drake-die-[1~4].png:150,units/undead/soulless-die-[5~10].png:150"
					[/frame]
				[/death]
				[attack_anim]
					direction="s"
					start_time=-200
					[filter_attack]
						name="touch"
					[/filter_attack]
					[frame]
						image="units/undead/zombie-drake-attack-s.png:400"
						sound="zombie-attack.wav"
					[/frame]
				[/attack_anim]
				[attack_anim]
					direction="n"
					start_time=-200
					[filter_attack]
						name="touch"
					[/filter_attack]
					[frame]
						image="units/undead/zombie-drake-attack-n.png:400"
						sound="zombie-attack.wav"
					[/frame]
				[/attack_anim]
				[attack_anim]
					direction="se,sw,ne,nw"
					start_time=-200
					[filter_attack]
						name="touch"
					[/filter_attack]
					[frame]
						image="units/undead/zombie-drake-attack.png:400"
						sound="zombie-attack.wav"
					[/frame]
				[/attack_anim]
				[trait]
					availability="musthave"
#textdomain wesnoth-help
					description=_"Fights normally during unfavorable times of day/night"
					female_name=_"female^fearless"
					id="fearless"
					male_name=_"fearless"
				[/trait]
				[resistance]
					arcane=140
				[/resistance]
				[attack]
					damage=6
#textdomain wesnoth-units
					description=_"touch"
					icon="attacks/touch-zombie.png"
					name="touch"
					number=2
					range="melee"
					type="impact"
					[specials]
						[plague]
#textdomain wesnoth-help
							description=_"When a unit is killed by a Plague attack, that unit is replaced with a Walking Corpse on the same side as the unit with the Plague attack. This doesn’t work on Undead or units in villages."
							id="plague"
							name=_"plague"
							type="Walking Corpse"
						[/plague]
					[/specials]
				[/attack]
				[defense]
					mountains=40
				[/defense]
			[/variation]
			[variation]
				advances_to="Soulless"
				alignment="chaotic"
				cost=8
#textdomain wesnoth-units
				description=_"Walking Corpses are the bodies of the dead, re-animated by dark magic. Though not especially dangerous to a trained soldier, the sight of one’s former comrades amongst their number is frightening to say the least." +
#textdomain wesnoth-help
					_"

Special Notes:" +
					_" Foes who lose their life to the plague will rise again in unlife, unless they are standing on a village."
