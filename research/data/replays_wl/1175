				id="amla_default"
				image="misc/icon-amla-tough.png"
				max_times=100
				strict_amla=yes
				[effect]
					apply_to="hitpoints"
					heal_full=yes
					increase_total=3
				[/effect]
				[effect]
					apply_to="max_experience"
					increase="20%"
				[/effect]
				[effect]
					apply_to="status"
					remove="poisoned"
				[/effect]
				[effect]
