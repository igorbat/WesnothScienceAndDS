								[/variable]
							[/or]
							[then]
								[item]
									image="units/saurians/skirmisher/skirmisher-ne-die4.png~TC($unit.side,magenta)"
									x="$x1"
									y="$y1"
								[/item]
							[/then]
							[else]
								[item]
									image="units/saurians/skirmisher/skirmisher-se-die4.png~TC($unit.side,magenta)"
									x="$x1"
									y="$y1"
								[/item]
							[/else]
						[/if]
					[/case]
					[case]
						value="Saurian Soothsayer,Saurian Bonecaster,EAE Saurian Soothsayer,EAE Saurian Bonecaster"
						[if]
							[variable]
								equals="n"
								name="unit.facing"
							[/variable]
							[or]
								[variable]
									equals="ne"
									name="unit.facing"
								[/variable]
							[/or]
							[or]
								[variable]
									equals="nw"
									name="unit.facing"
								[/variable]
							[/or]
							[then]
								[item]
									image="units/saurians/soothsayer/soothsayer-ne-die4.png~TC($unit.side,magenta)"
									x="$x1"
									y="$y1"
								[/item]
							[/then]
							[else]
								[item]
									image="units/saurians/soothsayer/soothsayer-se-die4.png~TC($unit.side,magenta)"
									x="$x1"
									y="$y1"
								[/item]
							[/else]
						[/if]
					[/case]
					[case]
						value="Woodsman,Bowman,Poacher,Longbowman,Master Bowman,Orcish Archer,Orcish Crossbowman,Orcish Slurbow,EAE Woodsman,EAE Bowman,EAE Poacher,EAE Longbowman,EAE Master Bowman,EAE Orcish Archer,EAE Orcish Crossbowman,EAE Orcish Slurbow"
						[item]
							image="units/undead-skeletal/archer-die-13.png~TC($unit.side,magenta)"
							x="$x1"
							y="$y1"
						[/item]
					[/case]
					[case]
						value="Halberdier,Dwarvish Ulfserker,Dwarvish Berserker,EAE Halberdier,EAE Dwarvish Ulfserker,EAE Dwarvish Berserker"
						[item]
							image="units/undead-skeletal/revenant-dying-7.png~TC($unit.side,magenta)"
							x="$x1"
							y="$y1"
						[/item]
					[/case]
					[case]
						value="Orcish Sovereign,Orcish Ruler,Orcish Leader,EAE Orcish Sovereign,EAE Orcish Ruler,EAE Orcish Leader"
