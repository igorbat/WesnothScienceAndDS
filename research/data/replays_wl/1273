			[/variable]
			[then]
				[message]
					message="Mirror! Sorry."
					speaker="narrator"
				[/message]
			[/then]
			[else]
				[message]
					message="Not a mirror. Have fun!"
					speaker="narrator"
				[/message]
			[/else]
		[/if]
		[clear_menu_item]
			id="mirroralert"
		[/clear_menu_item]
	[/event]
