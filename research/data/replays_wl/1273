						sound="wose-attack.ogg"
					[/attack_sound_frame]
				[/if]
				[else]
					hits=no
					[attack_sound_frame]
						sound="wose-miss.ogg"
					[/attack_sound_frame]
				[/else]
				[frame]
					image="units/woses/wose-attack-[1,2,1].png:[400,150,75]"
				[/frame]
			[/attack_anim]
			[death]
				start_time=0
