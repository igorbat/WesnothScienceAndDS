					name=_"Undead"
					random_leader="Dark Sorcerer,Revenant,Deathblade,Bone Shooter"
					recruit="Skeleton,Skeleton Archer,Walking Corpse,Ghost,Vampire Bat,Dark Adept,Ghoul"
					terrain_liked="Ss,Hh,Ha,Ww"
