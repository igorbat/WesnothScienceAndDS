							[/else]
						[/if]
					[/case]
					[case]
						value="Thief,EAE Thief"
						[if]
							[variable]
								equals="female"
								name="unit.gender"
							[/variable]
							[then]
								[item]
									image="units/human-outlaws/thief female-die-7.png~TC($unit.side,magenta)"
									x="$x1"
									y="$y1"
								[/item]
