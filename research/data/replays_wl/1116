	[/event]
	[event]
		name="prestart"
		[store_unit]
			kill=yes
			variable="leaders_to_make_quick"
			[filter]
				canrecruit=yes
				[filter_wml]
					max_moves=4
				[/filter_wml]
			[/filter]
		[/store_unit]
		[set_variable]
			name="i"
			value=0
		[/set_variable]
		[while]
			[variable]
				less_than="$leaders_to_make_quick.length"
				name="i"
			[/variable]
			[do]
				[if]
					[variable]
						boolean_equals=yes
						name="leaders_to_make_quick[$i].variables.dont_make_me_quick"
					[/variable]
					[then]
						[unstore_unit]
							variable="leaders_to_make_quick[$i]"
						[/unstore_unit]
					[/then]
					[else]
						[set_variables]
							name="temp"
							[literal]
								[trait]
#textdomain wesnoth-help
									female_name=_"female^quick"
									id="quick"
									male_name=_"quick"
									[effect]
										apply_to="movement"
										increase=1
									[/effect]
									[effect]
										apply_to="hitpoints"
										increase_total="-5%"
									[/effect]
								[/trait]
							[/literal]
						[/set_variables]
						[set_variables]
							mode="append"
							name="leaders_to_make_quick[$i].modifications.trait"
							[insert_tag]
								name="literal"
								variable="temp.trait"
							[/insert_tag]
						[/set_variables]
						[clear_variable]
							name="leaders_to_make_quick[$i].max_moves,leaders_to_make_quick[$i].moves,leaders_to_make_quick[$i].max_hitpoints,leaders_to_make_quick[$i].hitpoints"
						[/clear_variable]
						[unstore_unit]
							variable="leaders_to_make_quick[$i]"
						[/unstore_unit]
					[/else]
				[/if]
				[set_variable]
					add=1
					name="i"
				[/set_variable]
			[/do]
		[/while]
		[clear_variable]
			name="i"
		[/clear_variable]
		[clear_variable]
			name="leaders_to_make_quick,temp"
		[/clear_variable]
	[/event]
	[event]
		name="time over"
		[lua]
			code="
                local _ = wesnoth.textdomain ""wesnoth-multiplayer""
                local function all_sides()
                    local function f(s, i)
                        i = i + 1
                        local t = wesnoth.sides[i]
                        return t and i, t
                    end
                    return f, nil, 0
                end

                local income_factor = 5

                local side_num = -1
                local total_score = -1
                local side_comparison = """"
                for side, team in all_sides() do
                    repeat -- ugly hack to convert 'break' into 'continue'
                    if team.__cfg.hidden then
                        break
                    end
                    local r, g, b = 255, 255, 255
                    if     team.__cfg.color == 1 then r, g, b = 255,   0,   0
                    elseif team.__cfg.color == 2 then r, g, b =   0,   0, 255
                    elseif team.__cfg.color == 3 then r, g, b =   0, 255,   0
                    elseif team.__cfg.color == 4 then r, g, b = 155,  48, 255
                    elseif team.__cfg.color == 5 then r, g, b =   0,   0,   0
                    elseif team.__cfg.color == 6 then r, g, b = 165,  42,  42
                    elseif team.__cfg.color == 7 then r, g, b = 255, 165,   0
                    elseif team.__cfg.color == 8 then r, g, b = 255, 255, 255
                    elseif team.__cfg.color == 9 then r, g, b =   0, 128, 128 end
                    if # wesnoth.get_units( { side = side } ) == 0 then
                        side_comparison = side_comparison ..
                            string.format( tostring( _ ""<span strikethrough='true' foreground='#%02x%02x%02x'>Side %d</span>"") .. ""\n"",
                                r, g, b, side)
                        break
                    end
                    local income = team.total_income * income_factor
                    local units = 0
                    -- Calc the total unit-score here
                    for i, unit in ipairs( wesnoth.get_units { side = side } ) do
                        if not unit.__cfg.canrecruit then
                            wesnoth.fire(""unit_worth"", { id = unit.id })
                            units = units + wesnoth.get_variable(""unit_worth"")
                        end
                    end
                    -- Up to here
                    local total = units + team.gold + income
                    side_comparison = side_comparison ..
                        string.format( tostring( _ ""<span foreground='#%02x%02x%02x'>Side %d</span>:  Income score = %d  Unit score = %d  Gold = %d"") .. ""\n"" .. tostring( _ ""Grand total: <b>%d</b>"") .. ""\n"",
                            r, g, b, side, income, units, team.gold, total)
                    if total > total_score then
                        color = string.format(""#%02x%02x%02x"", r, g, b)
                        side_num = side
                        total_score = total
                    end
                    until true -- end ugly hack
                end
                side_comparison = side_comparison ..
                    string.format( ""\n"" .. tostring( _ ""<span foreground='%s'>Side %d</span> has the advantage.""), color, side_num)
                wesnoth.fire(""message"", { message = side_comparison, speaker = ""narrator"", image = ""wesnoth-icon.png""})
            "
		[/lua]
	[/event]
	[event]
		name="prestart"
		[set_variable]
			name="pya_display_help"
			value=yes
		[/set_variable]
	[/event]
	[event]
		first_time_only=no
		name="preload"
		[lua]
			code="if not wesnoth then
	package.path = package.path .. "";../../../../../data/lua/?.lua""
	wesnoth = require(""wesnoth"")
end
local _ = wesnoth.textdomain(""wesnoth-Plan_Your_Advancements_Mod"")
-- #textdomain wesnoth-Plan_Your_Advancements_Mod


local scale_factor = 50

function plan_your_advancements_mod_main()

end

function plan_your_advancements_mod_has_advance_choice(unit)
	return string.find(unit.__cfg.advances_to, "","") or unit.variables.pya_original_advances_to
end

function plan_your_advancements_mod_set_advances_to()
	local helper = wesnoth.require(""lua/helper.lua"")
	local event_context = wesnoth.current.event_context
	local unit = wesnoth.get_units({ x = event_context.x1, y = event_context.y1 })[1]
	local dump = unit.__cfg

	local choices = {}
	local type_ids = {}
	local choices_string = unit.variables.pya_original_advances_to or dump.advances_to
	for type_id in string.gmatch(choices_string, ""[^%s,][^,]*"") do
		table.insert(type_ids, type_id)
		local type = wesnoth.unit_types[type_id].__cfg
		table.insert(choices, string.format(""&%s~SCALE(%u, %u)=%s"", type.image, scale_factor, scale_factor, tostring(type.name)))
		if unit.variables.pya_original_advances_to and type_id == dump.advances_to then choices[#choices] = ""*"" .. choices[#choices] end
	end
	table.insert(choices, string.format(""&units/random-dice.png~SCALE(%u, %u)=%s"", scale_factor, scale_factor, tostring(_""random"")))
	if not unit.variables.pya_original_advances_to then choices[#choices] = ""*"" .. choices[#choices] end
	table.insert(choices, string.format(""&icons/arrows/arrows_blank_left_30.png~SCALE(%u, %u)=%s"", scale_factor, scale_factor, tostring(_""cancel"")))

	local settings = {
		speaker = ""narrator"",
		caption = _""plan an advancement"",
		image = ""icons/icon-addon-publish.png~SCALE(160,160)"",
		message = _""What should our unit become if/when it will advance ?""
	}
	if unit.variables.pya_original_advances_to then
		settings.message = string.format(""%s %s.\n%s"",
		tostring(_""Our unit is currently set to advance to""), tostring(wesnoth.unit_types[dump.advances_to].name), tostring(settings.message))
	end
	local choice = helper.get_user_choice(settings, choices)

	if choice <= #type_ids then
		local original_advances_to = dump.advances_to
		dump.advances_to = type_ids[choice]
		wesnoth.put_unit(dump)
		if not unit.variables.pya_original_advances_to then
			unit.variables.pya_original_advances_to = original_advances_to
		end
	elseif choice == #type_ids + 1 and unit.variables.pya_original_advances_to then
		dump.advances_to = unit.variables.pya_original_advances_to
		wesnoth.put_unit(dump)
		unit.variables.pya_original_advances_to = nil
	end
end


"
		[/lua]
		[lua]
			code="if not wesnoth then
	package.path = package.path .. "";../../../../../data/lua/?.lua""
	wesnoth = require(""wesnoth"")
end

function wesnoth.wml_actions.pya_check_version(cfg)
	local function version_is_sufficient(required)
		if not wesnoth.compare_versions then return false end
		return wesnoth.compare_versions(wesnoth.game_config.version, "">="", required)
	end
	if not version_is_sufficient(cfg.required) then wesnoth.fire_event(""pya_wrong_bfw_version_message"") end
end

"
		[/lua]
	[/event]
	[event]
		name="turn 1"
		[set_variable]
			name="pya_bfw_version_suffices"
			value=yes
		[/set_variable]
		[pya_check_version]
			required="1.11.10"
		[/pya_check_version]
		[fire_event]
			name="pya_mod_turn_one"
		[/fire_event]
	[/event]
	[event]
		name="pya_mod_turn_one"
		[filter_condition]
			[variable]
				equals=yes
				name="pya_bfw_version_suffices"
			[/variable]
		[/filter_condition]
		[clear_variable]
			name="pya_bfw_version_suffices"
		[/clear_variable]
		[if]
			[variable]
				boolean_equals=yes
				name="pya_display_help"
			[/variable]
			[then]
				[message]
#textdomain wesnoth-Plan_Your_Advancements_Mod
					caption=_"Plan Your Advancements Mod"
					image="wesnoth-icon.png"
					message=_"To plan what a unit will advance to, right-click it while it is your turn. The menu is only available if there are at least two possible choices. The decision can be undone (set back to random) or changed. When the unit advances it will become what you chose, especially if this happens while it is not your turn."
					side_for=""
					speaker="narrator"
				[/message]
			[/then]
		[/if]
		[set_menu_item]
			description=_"show PYA mod help"
			id="pya_help"
			[command]
				[message]
					caption=_"Plan Your Advancements Mod"
					image="wesnoth-icon.png"
					message=_"To plan what a unit will advance to, right-click it while it is your turn. The menu is only available if there are at least two possible choices. The decision can be undone (set back to random) or changed. When the unit advances it will become what you chose, especially if this happens while it is not your turn."
					side_for="$side_number"
					speaker="narrator"
				[/message]
			[/command]
		[/set_menu_item]
		[set_menu_item]
			description=_"plan advancement"
			id="pya_plan_advancement"
			[show_if]
				[have_unit]
					lua_function="plan_your_advancements_mod_has_advance_choice"
					side="$side_number"
					x="$x1"
					y="$y1"
				[/have_unit]
			[/show_if]
			[command]
				[lua]
					code="plan_your_advancements_mod_set_advances_to()"
				[/lua]
			[/command]
		[/set_menu_item]
	[/event]
	[event]
		name="pya_wrong_bfw_version_message"
