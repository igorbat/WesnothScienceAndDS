						affect_self=yes
						description=_"The unit will heal itself 8 HP per turn. If it is poisoned, it will remove the poison instead of healing."
						female_name=_"female^regenerates"
						id="fake_regenerates"
						name=_"regenerates"
						poison="cured"
						value=8
					[/regenerate]
				[/abilities]
			[/effect]
		[/object]
	[/event]
	[event]
		first_time_only=no
		name="turn refresh"
		[object]
			silent=yes
			[filter]
				side="$side_number"
