				condition="win"
#textdomain wl_mappack
				description=_"Kill the enemy leader"
			[/objective]
		[/objectives]
		[capture_village]
			side=1
			x=33
			y=18
		[/capture_village]
		[capture_village]
			side=2
			x=48
			y=19
		[/capture_village]
