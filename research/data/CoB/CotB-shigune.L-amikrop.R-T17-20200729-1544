[switch]
variable="Rav_Color_Blue"
[case]
value="custom"
[set_variable]
name="Rav_Color_custom_Blue"
value="yes"
[/set_variable]
[/case]
[case]
value="core"
[set_variable]
name="Rav_Color_Light_Blue"
value="yes"
[/set_variable]
[/case]
