					[do]
						[set_variable]
							name="this_item.advances_to"
							value="Exi Slitherblade"
						[/set_variable]
						[unstore_unit]
							find_vacant=no
							variable="this_item"
						[/unstore_unit]
					[/do]
				[/foreach]
				[clear_variable]
					name="MODIFY_UNIT_store"
				[/clear_variable]
			[/then]
		[/if]
	[/event]
