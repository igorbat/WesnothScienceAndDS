						[unstore_unit]
							find_vacant=no
							variable="this_item"
						[/unstore_unit]
					[/do]
				[/foreach]
				[clear_variable]
					name="MODIFY_UNIT_store"
				[/clear_variable]
			[/then]
		[/if]
	[/event]
	[event]
		first_time_only=no
		id="samuraifordrake"
