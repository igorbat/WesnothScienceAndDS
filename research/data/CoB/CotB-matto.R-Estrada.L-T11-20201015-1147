	[event]
		first_time_only=no
		id="generalcanmarshal"
		name="recruit,recall,post advance,unit placed"
		[filter]
			type="General"
			x="$x1"
			y="$y1"
		[/filter]
		[if]
			[variable]
				equals=yes
				name="EXI_GrandMarshalEnable"
			[/variable]
			[then]
				[store_unit]
					kill=yes
					variable="MODIFY_UNIT_store"
					[filter]
						x="$x1"
						y="$y1"
					[/filter]
				[/store_unit]
				[foreach]
					array="MODIFY_UNIT_store"
					[do]
						[set_variable]
							name="this_item.advances_to"
							value="Grand Marshal"
						[/set_variable]
						[unstore_unit]
							find_vacant=no
							variable="this_item"
						[/unstore_unit]
					[/do]
				[/foreach]
				[clear_variable]
					name="MODIFY_UNIT_store"
				[/clear_variable]
			[/then]
		[/if]
	[/event]
